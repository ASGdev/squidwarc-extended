{
  "use": "puppeteer",
  "headless": false,
  "script": "./userFns.js",
  "depth": 1,
  "seeds": [
    "http://jtconcept.free.fr/"
  ],
  "warc": {
    "naming": "url",
	"output": "./output/"
  },
  "connect": {
    "launch": true,
    "host": "localhost",
    "port": 9222
  },
  "crawlControl": {
    "globalWait": 60000,
    "inflightIdle": 1000,
    "numInflight": 2,
    "navWait": 8000,
	"mode": "page-same-subdomain",
    "depth": 9999999999
  }
}

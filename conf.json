{
  "use": "puppeteer",
  "headless": false,
  "script": "./userFns.js",
  "seeds": [
    "http://jtconcept.free.fr/"
  ],
  "warc": {
    "naming": "url"
  },
  "connect": {
    "launch": true,
    "host": "localhost",
    "port": 9222
  },
  "crawlControl": {
    "globalWait": 60000,
    "inflightIdle": 1000,
    "numInflight": 2,
    "navWait": 8000,
	"mode": "page-same-domain",
    "depth": 9999999999
  },
  "logging": {
    "file": {
	  "enabled": true,
	  "path": "./output/text.txt"
	},
	"http": {
	  "enabled": true,
	  "endpoint": "http://localhost:3001/",
	  "apikey": "my-api-key"
	}
  }
}
